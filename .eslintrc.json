{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb-base"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "indent": "off",
        "import/prefer-default-export": "off",
        "comma-dangle": "off",
        "import/extensions": "off"
    }
}
